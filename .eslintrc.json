{
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "commonjs": true,
      "node": true,
      "es6": true,
      "jest": false,
      "jquery": true
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "no-console": "off",
      "strict": ["error", "global"],
      "curly": "warn"
    },
    "overrides": [
      {
        "files": [
          "src/**/*.js"
        ],
        "extends": ["react-app", "eslint:recommended"]
      }
    ]
  }